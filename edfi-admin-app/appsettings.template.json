{
    "AppSettings": {
        "AppStartup": "OnPrem",
        "DatabaseEngine": "PostgreSQL",
        "ApplicationInsightsInstrumentationKey": "",
        "XsdFolder": "/app/Schema",
        "DefaultOdsInstance": "EdFi ODS",
        "ProductionApiUrl": "$API_URL",
        "ApiExternalUrl" : "$API_URL",
        "SystemManagedSqlServer": "true",
        "DbSetupEnabled": "false",
        "SecurityMetadataCacheTimeoutMinutes": "0",
        "ApiStartupType": "YearSpecific",
        "LocalEducationAgencyTypeValue": "Local Education Agency",
        "SchoolTypeValue": "School",
        "BulkUploadHashCache": "/app/BulkUploadHashCache/",
        "IdaAADInstance": "",
        "IdaClientId": "",
        "IdaClientSecret": "",
        "IdaTenantId": "",
        "IdaSubscriptionId": "",
        "AwsCurrentVersion": "1.0",
        "EncryptionProtocol": "AES",
        "EncryptionKey": "$ENCRYPTION_KEY"
    },
    "ConnectionStrings": {
      "Admin": "host=$DB_HOST;port=5432;username=postgres;password=$DB_PASS;sslmode=Require;database=EdFi_Admin;Application Name=EdFi.Ods.AdminApp",
      "Security": "host=$DB_HOST;port=5432;username=postgres;password=$DB_PASS;sslmode=Require;database=EdFi_Security;Application Name=EdFi.Ods.AdminApp",
      "ProductionOds": "host=$DB_HOST;port=5432;username=postgres;password=$DB_PASS;sslmode=Require;database=EdFi_{0};Application Name=EdFi.Ods.AdminApp"
    },
    "Log4NetCore": {
        "Log4NetConfigFileName": "./log4net.config"
    },
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft": "Warning",
            "Microsoft.Hosting.Lifetime": "Information"
        }
    },
    "AllowedHosts": "*"
  }